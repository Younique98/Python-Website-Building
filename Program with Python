first = 'John'
last = 'Smith'
message = first + ' [' + last + '] is a coder'
msg = f'{first} [{last}] is a coder'
print(msg)

course = 'Python for Beginners'
print(len(course))
print(course.upper())

course = 'Python for Beginners'
print(len(course))
print(course.upper())
print(course.lower())

course = 'Python for Beginners'
